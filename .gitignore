# Ignore Node.js dependencies
node_modules/

# Ignore log files
logs/
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore environment variable files
.env
.env.local
.env.development
.env.test
.env.production

# Ignore OS-generated files
.DS_Store          # macOS
Thumbs.db          # Windows

# Ignore build output (if you use a build step)
dist/
build/

# Ignore editor-specific files
.vscode/           # VS Code settings
.idea/             # IntelliJ IDEA settings
*.sublime-*        # Sublime Text files

# Ignore package lock files (optional, depending on preference)
package-lock.json  # npm lock file
yarn.lock          # Yarn lock file

# Ignore temporary files
*.tmp
*.temp
